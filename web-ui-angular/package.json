{
  "name": "web-ui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "init-deployments": "mkdir -p ./eth/deployed && cp -r ../my-deployments/ ./eth/deployed",
    "sync-deployments": "cp ../artifacts/contracts/deployed/* ./eth/deployed",
    "dev-update-contract": "nodemon ./scripts/dev-update-contract.js --watch eth/deployed/ganache",
    "start": "npm run init-deployments && (npm run dev-update-contract & ng serve)",
    "prod-update-contract": "node ./scripts/prod-update-contract.js",
    "build": "npm run setup-contract-for-build && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@material-design-icons/font": "^0.9.1",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/jazzicon": "^2.0.0",
    "ethers": "^5.4.7",
    "evm-chains": "^0.2.0",
    "mersenne-twister": "^1.1.0",
    "ngx-clipboard": "^14.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copy-and-watch": "latest",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.13",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
